<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.semproject.shoppingnow">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShopNow">
        <activity
            android:name="com.semproject.shoppingnow.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.ShopNow.NoActionBar">

        </activity>
        <activity
            android:name="com.semproject.shoppingnow.SplashActivity"
            android:theme="@style/Theme.ShopNow.NoAppBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>